<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="yunstudio2015.android.yunmeet">

    <uses-permission android:name="android.permission.INTERNET" />
    <!-- Include following permission if you want to cache images on SD card -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <application
        android:name=".app.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activityz.MainActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".activityz.RestingActivity"
            android:label="@string/title_activity_resting" />
        <activity
            android:name=".activityz.ShowActivity"
            android:label="@string/title_activity_show" />
        <activity android:name=".activityz.SignupActivity" />
        <activity android:name=".activityz.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activityz.TestingShowActivity"
            android:label="@string/title_activity_testing_show" />
        <activity android:name=".activityz.ForgetPasswordActivity" />
        <activity android:name=".activityz.SetNickNameActivity" />
        <activity android:name=".activityz.SetFaceActivity"></activity>
    </application>

</manifest>
