<resources xmlns:tools="http://schemas.android.com/tools">

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/actionbar_color</item>
        <item name="colorPrimaryDark">@color/actionbar_color</item>
    </style>


    <style name="whitetext" parent="@android:style/TextAppearance">
        <!-- Customize your theme here. -->
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">14sp</item>
    </style>


    <color name="white1">#dddddd</color>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="FloatingActionButton">
        <item name="elevation">6dp</item>
        <item name="fabSize">normal</item>
        <item name="pressedTranslationZ">12dp</item>
        <item name="borderWidth">0dp</item>
    </style>
</resources>
